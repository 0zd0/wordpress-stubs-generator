{
    "name": "onepix/wordpress-stubs-generator",
    "description": "Generate stubs from any PHP code for IDE completion and static analysis.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "IDE",
        "static analysis"
    ],
    "homepage": "https://github.com/0zd0/wordpress-stubs-generator",
    "authors": [
        {
            "name": "Onepix",
            "email": "info@onepix.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "StubsGenerator\\": "src/"
        }
    },
    "replace": {
        "php-stubs/generator": "*"
    },
    "bin": [
        "bin/generate-stubs"
    ],
    "require": {
        "php": "^8.1",
        "nikic/php-parser": "^5.3",
        "symfony/filesystem": "^6.4",
        "symfony/finder": "^6.4",
        "symfony/console": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "friendsofphp/php-cs-fixer": "^3.65"
    },
    "scripts": {
        "test": "phpunit",
        "cs": "php-cs-fixer fix --dry-run",
        "cs:fix": "php-cs-fixer fix",
        "check": [
            "@cs",
            "@test"
        ]
    }
}
